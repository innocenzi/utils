{
  "name": "@innocenzi/utils",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "version": "0.2.6",
  "packageManager": "pnpm@9.15.0",
  "description": "Opinionated collection of TypeScript utilities",
  "author": "Enzo Innocenzi <enzo@innocenzi.dev>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/innocenzi",
  "homepage": "https://github.com/innocenzi/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/innocenzi/utils.git"
  },
  "bugs": {
    "url": "https://github.com/innocenzi/utils/issues"
  },
  "keywords": [
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nr build --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit \"release: v\" && npm publish",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "p-limit": "^6.1.0",
    "throttle-debounce": "5.0.2"
  },
  "devDependencies": {
    "@innocenzi/eslint-config": "^0.22.4",
    "@types/node": "^20.17.9",
    "@types/throttle-debounce": "^5.0.2",
    "bumpp": "^9.9.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  }
}
