{
    "name": "@innocenzi/utils",
    "publishConfig": {
        "access": "public"
    },
    "version": "0.2.3",
    "description": "Opinionated collection of TypeScript utilities",
    "keywords": [
        "utils"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/innocenzi/utils.git"
    },
    "funding": "https://github.com/sponsors/innocenzi",
    "author": "Enzo Innocenzi <enzo@innocenzi.dev>",
    "type": "module",
    "packageManager": "pnpm@8.6.12",
    "homepage": "https://github.com/innocenzi/utils#readme",
    "bugs": {
        "url": "https://github.com/innocenzi/utils/issues"
    },
    "sideEffects": false,
    "files": [
        "dist",
        "*.d.ts"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "unbuild",
        "dev": "nr build --watch",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "prepublishOnly": "npm run build",
        "release": "bumpp --commit \"release: v\" && npm publish",
        "start": "esno src/index.ts",
        "typecheck": "tsc --noEmit",
        "test": "vitest"
    },
    "dependencies": {
        "p-limit": "^4.0.0",
        "throttle-debounce": "5.0.0"
    },
    "devDependencies": {
        "@innocenzi/eslint-config": "^0.20.0",
        "@types/node": "^20.5.6",
        "@types/throttle-debounce": "^5.0.0",
        "bumpp": "^9.2.0",
        "eslint": "^8.48.0",
        "typescript": "^5.2.2",
        "unbuild": "^2.0.0",
        "vite": "^4.4.9",
        "vitest": "^0.34.3"
    }
}
